{
	"name": "@natlibfi/melinda-marc-record-merge-reducers",
	"type": "module",
	"description": "Generic MARC record merge reducers for use with marc-record-merge",
	"author": {
		"name": "The National Library of Finland"
	},
	"keywords": [],
	"homepage": "https://github.com/natlibfi/melinda-marc-record-merge-reducers-js",
	"bugs": {
		"url": "https://github.com/natlibfi/melinda-marc-record-merge-reducers-js/issues"
	},
	"repository": {
		"type": "git",
		"url": "git@github.com:natlibfi/melinda-marc-record-merge-reducers-js.git"
	},
	"license": "MIT",
	"version": "3.1.0-alpha.4",
	"main": "./dist/index.js",
	"engines": {
		"node": ">=22"
	},
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"prepare": "npm run build",
		"start": "node dist/index.js",
		"cli": "node dist/cli.js",
		"lint": "eslint src",
		"lint:dev": "eslint --fix src",
		"test:base": "node --test --test-force-exit --experimental-test-coverage --test-reporter=spec",
		"test:base:only": "node --test --test-only --test-force-exit --experimental-test-coverage --test-reporter=spec './test/**/*.test.js'",
		"dev:test": "cross-env DEBUG=@natlibfi/* NODE_ENV=test node --watch --test --experimental-test-coverage --test-reporter=spec './test/**/*.test.js'",
		"dev:test:only": "cross-env DEBUG=@natlibfi/* NODE_ENV=test node --watch --test --test-only --experimental-test-coverage --test-reporter=spec './test/**/*.test.js'",
		"test": "npm run lint && npm run test:base",
		"test:only": "npm run lint && npm run test:base:only",
		"build": "esbuild \"src/**/*.js\" --sourcemap --platform=node --outdir=dist",
		"watch:test": "cross-env DEBUG=1 NODE_ENV=test nodemon -w src -w test-fixtures --exec 'npm run test:dev'"
	},
	"nv-mnemonics": {
		"@natlibfi/whatever-uses-branch": "github:natlibfi/marc-record-validators-melinda#branchName"
	},
	"dependencies": {
		"@natlibfi/marc-record": "^10.0.0-alpha.1",
		"@natlibfi/marc-record-merge": "^8.0.0-alpha.1",
		"@natlibfi/marc-record-validate": "^9.0.0-alpha.1",
		"@natlibfi/marc-record-validators-melinda": "^12.0.0-alpha.4",
		"clone": "^2.1.2",
		"debug": "^4.4.3",
		"isbn3": "^1.2.15"
	},
	"devDependencies": {
		"@natlibfi/fixugen": "^3.0.0-alpha.6",
		"@natlibfi/fixura": "^4.0.0-alpha.13",
		"cross-env": "^10.1.0",
		"esbuild": "^0.25.11",
		"eslint": "^9.37.0"
	},
	"overrides": {
		"nanoid": "^3.3.8"
	}
}
