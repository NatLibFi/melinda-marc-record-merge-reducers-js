{
	"name": "@natlibfi/melinda-marc-record-merge-reducers",
	"description": "Generic MARC record merge reducers for use with marc-record-merge",
	"author": {
		"name": "The National Library of Finland"
	},
	"keywords": [],
	"homepage": "https://github.com/natlibfi/melinda-marc-record-merge-reducers-js",
	"bugs": {
		"url": "https://github.com/natlibfi/melinda-marc-record-merge-reducers-js/issues"
	},
	"repository": {
		"type": "git",
		"url": "git@github.com:natlibfi/melinda-marc-record-merge-reducers-js.git"
	},
	"license": "MIT",
	"version": "2.4.0-alpha.1",
	"main": "./dist/index.js",
	"engines": {
		"node": ">=20"
	},
	"publishConfig": {
		"access": "public"
	},
	"scripts": {
		"prepare": "npm run build",
		"start": "node dist/index.js",
		"cli": "node dist/cli.js",
		"lint": "eslint src",
		"lint:dev": "eslint --fix src",
		"test:base": "cross-env NODE_ENV=test nyc mocha --require @babel/register",
		"test": "npm run lint && npm run test:base -- --recursive src",
		"test:dev": "npm run lint:dev && npm run test:base -- --recursive src && npm run coverage",
		"coverage": "nyc check-coverage --per-file",
		"build": "babel src --source-maps --copy-files --delete-dir-on-start --out-dir=dist",
		"watch:test": "cross-env DEBUG=1 NODE_ENV=test nodemon -w src -w test-fixtures --exec 'npm run test:dev'"
	},
	"dependencies": {
		"@natlibfi/marc-record": "^9.1.6",
		"@natlibfi/marc-record-merge": "^7.0.10",
		"@natlibfi/marc-record-validate": "^8.0.14",
		"@natlibfi/marc-record-validators-melinda": "^11.6.7",
		"debug": "^4.4.1",
		"isbn3": "^1.2.13"
	},
	"devDependencies": {
		"@babel/cli": "^7.28.0",
		"@babel/core": "^7.28.0",
		"@babel/node": "^7.28.0",
		"@babel/preset-env": "^7.28.0",
		"@babel/register": "^7.27.1",
		"@natlibfi/fixugen": "^2.0.15",
		"@natlibfi/fixura": "^3.0.14",
		"babel-plugin-istanbul": "^7.0.0",
		"babel-plugin-rewire": "^1.2.0",
		"chai": "^4.5.0",
		"cross-env": "^7.0.3",
		"eslint": "^9.33.0",
		"mocha": "^11.7.1",
		"nodemon": "^3.1.9",
		"nyc": "^17.1.0"
	},
	"babel": {
		"presets": [
			[
				"@babel/preset-env",
				{
					"targets": {
						"node": "20"
					}
				}
			]
		],
		"env": {
			"test": {
				"plugins": [
					"istanbul",
					"rewire"
				]
			}
		}
	},
	"nyc": {
		"exclude": [
			"*/*.spec.js",
			"*/**/*.spec.js"
		],
		"reporter": [
			"lcov",
			"html"
		],
		"require": [
			"@babel/register"
		],
		"sourceMap": false,
		"instrument": false,
		"lines": 80,
		"statements": 80,
		"functions": 80,
		"branches": 80
	},
	"overrides": {
		"nanoid": "^3.3.8"
	}
}
